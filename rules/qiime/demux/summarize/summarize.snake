rule qiime__demux_summarize:
    input:
        qza     = expand('reads/qiime/{name}.qza',
                        name=method_config['name'])
    output:
        qzv     = expand('reads/qiime/{name}.qzv',
                        name=method_config['name'])
    threads:
        int(config['threads'])
    shell:
        '''
        qiime demux summarize \
          --i-data {input.qza} \
          --o-visualization {output.qzv}
        '''
